@model Fusion.Models.CallCenter.CallCenterModels.RestReport
@{
    ViewBag.Title = "Список для прозвона";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Находка</h2>
<div class="row">
    <div class="col-md-13">
        <table class="table table-striped table-bordered table-hover" style="font-size:10px!important">
            <tr>
                <th>Число</th>
                <th>Час</th>
                <th>Сумма заказа</th>
                <th>Количество</th>
                <th>Тип</th>
            </tr>
            @if (Model != null)
            {
                foreach (var r in Model.rs)
                {
                    <tr>
                        <td>@r.day</td>
                        <td>@r.vremya</td>
                        <td>@r.order_summ</td>
                        <td>@r.order_cnt</td>
                        <td>@r.DlvType</td>
                    </tr>
                }
            }
        </table>
    </div>
</div>